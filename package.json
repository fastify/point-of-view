{
  "name": "point-of-view",
  "version": "1.0.0",
  "description": "Template plugin for Fastify",
  "main": "index.js",
  "scripts": {
    "example": "node example.js",
    "example-with-options": "node example-ejs-with-some-options.js",
    "test-with-snapshot": "standard && cross-env TAP_SNAPSHOT=1 tap test-with-snapshot.js",
    "test": "standard && tap test.js test-with-snapshot.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fastify/point-of-view.git"
  },
  "keywords": [
    "fastify",
    "template",
    "view",
    "speed",
    "ejs",
    "nunjucks",
    "pug",
    "handlebars"
  ],
  "author": "Tomas Della Vedova - @delvedor (http://delved.org)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fastify/point-of-view/issues"
  },
  "homepage": "https://github.com/fastify/point-of-view#readme",
  "dependencies": {
    "fastify-plugin": "^0.2.2",
    "hashlru": "^2.2.1"
  },
  "devDependencies": {
    "cross-env": "^5.1.3",
    "ejs": "^2.5.7",
    "ejs-mate": "^2.3.0",
    "express": "^4.16.2",
    "fastify": "^1.1.0",
    "handlebars": "^4.0.11",
    "marko": "^4.7.5",
    "nunjucks": "^3.1.2",
    "pre-commit": "^1.2.2",
    "pug": "^2.0.0",
    "request": "2.83.0",
    "standard": "^11.0.0",
    "tap": "^11.1.1"
  },
  "standard": {
    "ignore": [
      "*.marko.js"
    ]
  }
}
